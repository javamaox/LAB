<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.qtrmoon.sysManage.bean.Module" table="SYS_MODULE">
        <id name="id" type="java.lang.String">
            <column name="ID" />
            <generator class="assigned" />
        </id>
        <property name="module" type="java.lang.String">
            <column name="MODULE" />
        </property>
        <property name="funcs" type="java.lang.String">
            <column name="FUNCS" />
        </property>
        <property name="pic" type="java.lang.String">
            <column name="PIC" />
        </property>
        <property name="name" type="java.lang.String">
            <column name="NAME" />
        </property>
        <property name="link" type="java.lang.String">
            <column name="LINK" />
        </property>
        <property name="info" type="java.lang.String">
            <column name="INFO" />
        </property>
        <property name="ord" type="java.lang.String">
            <column name="ORD" />
        </property>
    </class>
</hibernate-mapping>
